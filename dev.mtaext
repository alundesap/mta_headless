_schema-version: '3.1'
ID: headless-mtaex
extends: headless

modules:
 - name: headless-utl
   parameters:
     app-name: headless_utl
     host: headless-utl-${space}
     domain: conciletime.com

 - name: headless-chr
   parameters:
     app-name: headless_chr
     host: headless-chr-${space}
     domain: conciletime.com
     
 - name: headless-web
   parameters:
     app-name: headless_web
     host: conciletime-web-${space} # Must be <subaccount subdomain>-web-${space} where subaccount is where you are deploying into
     domain: conciletime.com
   properties:
     TENANT_HOST_PATTERN: '^(.*)-web-${space}.conciletime.com'
     
resources:
 - name: headless-uaa
   parameters:
     path: xs-security.json # Must include redirect-uris that match domain enabled where you are deploying into
     service-name: HEADLESS_UAA
     service-plan: application
     service: xsuaa
     shared: true
     config:
       xsappname: headless
       tenant-mode: shared

 - name: headless-ss
   parameters:
     service-name: HEADLESS_SS
